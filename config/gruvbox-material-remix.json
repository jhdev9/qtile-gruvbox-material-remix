{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "right",
      "segments": [
        {
          "background": "#72966c",
          "foreground": "#FBFBFB",
          "leading_diamond": "\ue0b6",
          "properties": {
            "style": "folder"
          },
          "style": "diamond",
          "template": "\ue5ff {{ .Path }}",
          "type": "path",
          "trailing_diamond": "\ue0b4 "
        },
        {
          "background": "#bd6f3e",
          "foreground": "#FBFBFB",
          "leading_diamond": " \ue0b6",
          "properties": {
            "branch_icon": ""
          },
          "style": "diamond",
          "template": "{{ .HEAD }}",
          "type": "git"
        },
        {
          "background": "#68948a",
          "foreground": "#FBFBFB",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue235{{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "type": "python"
        },
        {
          "background": "#6B92BB",
          "foreground": "#FBFBFB",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue626{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "go"
        },
        {
          "background": "#5F9A52",
          "foreground": "#FBFBFB",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue718{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "type": "node"
        },
        {
          "background": "#424242",
          "foreground": "#E64747",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue791{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "ruby"
        },
        {
          "background": "#424242",
          "foreground": "#E64747",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue738{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "java"
        },
        {
          "background": "#424242",
          "foreground": "#9B6BDF",
          "leading_diamond": " \ue0b6",
          "properties": {
            "fetch_version": false
          },
          "style": "diamond",
          "template": "\ue624{{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "type": "julia"
        },
          {
          "background": "#b85651",
          "foreground": "#FBFBFB",
          "leading_diamond": " \ue0b6",
           "properties": {
            "always_enabled": true
          },
          "style": "diamond",
          "template": " \ueba2 {{ .FormattedMs }} ",
          "type": "executiontime",
          "trailing_diamond": "\ue0b4"
        }
      ],
      "type": "rprompt"
    },
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#72966c",
          "foreground": "#FBFBFB",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{ .UserName }} \u276f",
          "trailing_diamond": "\ue0b4 ",
          "type": "session"
        }
      ],
      "type": "prompt"
    }
  ],
  "version": 2
}

